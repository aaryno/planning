   "source": [
    "def calculate_topographic_metrics_complete(dem_path, output_dir=None):\n",
    "    \"\"\"\n",
    "    Complete implementation of the topographic metrics function.\n",
    "    \n",
    "    This is the function you need to implement in your assignment!\n",
    "    \"\"\"\n",
    "    import rasterio\n",
    "    import numpy as np\n",
    "    from pathlib import Path\n",
    "    \n",
    "    print(\"🗻 Starting topographic analysis...\")\n",
    "    \n",
    "    # STEP 1: Open DEM and read elevation data\n",
    "    print(\"📖 Step 1: Reading DEM data\")\n",
    "    with rasterio.open(dem_path) as src:\n",
