    "    axes[0,0].set_ylabel('Pixel Y')\n",
    "    cbar1 = plt.colorbar(im1, ax=axes[0,0], shrink=0.8)\n",
    "    cbar1.set_label('Elevation (meters)', rotation=270, labelpad=20)\n",
    "    \n",
    "    # Plot 2: Hillshade effect\n",
    "    from matplotlib.colors import LightSource\n",
    "    ls = LightSource(azdeg=315, altdeg=45)\n",
    "    hillshade = ls.hillshade(elevation, vert_exag=0.01, dx=30, dy=30)\n",
    "    axes[0,1].imshow(hillshade, cmap='gray', aspect='equal')\n",
    "    axes[0,1].set_title('Hillshade Visualization', fontsize=14, fontweight='bold')\n",
