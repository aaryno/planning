    "# Example 4: Efficient window-based processing\n",
    "def example_window_processing():\n",
    "    \"\"\"\n",
    "    Demonstrate efficient processing using windows\n",
    "    \"\"\"\n",
    "    \n",
    "    print(\"\\n=== WINDOW-BASED PROCESSING ===\")\n",
    "    \n",
    "    with rasterio.open(sample_raster) as src:\n",
    "        print(f\"Processing raster with {src.count} bands, shape: {src.shape}\")\n",
    "        \n",
    "        # Define processing parameters\n",
    "        window_size = 50  # Process in 50x50 pixel chunks\n",
    "        overlap = 5       # 5-pixel overlap between windows\n",
    "        \n",
    "        # Calculate statistics for each window\n",
    "        window_stats = []\n",
    "        \n",
    "        for row_start in range(0, src.height, window_size - overlap):\n",
    "            for col_start in range(0, src.width, window_size - overlap):\n",
    "                \n",
    "                # Define window bounds\n",
    "                row_end = min(row_start + window_size, src.height)\n",
    "                col_end = min(col_start + window_size, src.width)\n",
    "                \n",
    "                # Create window\n",
    "                from rasterio.windows import Window\n",
    "                window = Window(col_start, row_start, col_end - col_start, row_end - row_start)\n",
    "                \n",
    "                # Read window data\n",
    "                window_data = src.read(1, window=window)\n",
    "                \n",
    "                # Calculate statistics for this window\n",
    "                if window_data.size > 0:\n",
    "                    window_bounds = src.window_bounds(window)\n",
    "                    \n",
    "                    stats = {\n",
    "                        'window': window,\n",
    "                        'bounds': window_bounds,\n",
    "                        'mean_elevation': float(np.mean(window_data)),\n",
    "                        'max_elevation': float(np.max(window_data)),\n",
    "                        'min_elevation': float(np.min(window_data)),\n",
    "                        'pixel_count': int(window_data.size)\n",
    "                    }\n",
    "                    \n",
    "                    window_stats.append(stats)\n",
