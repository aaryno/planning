    "\n",
    "plt.scatter(all_dates, all_clouds, alpha=0.3, s=20, label='All scenes', color='gray')\n",
    "if len(summer_df) > 0:\n",
    "    plt.scatter(summer_df['datetime'], summer_df['cloud_cover'], \n",
    "               s=40, label='Summer <20%', color='orange', edgecolor='black')\n",
    "if len(winter_df) > 0:\n",
    "    plt.scatter(winter_df['datetime'], winter_df['cloud_cover'], \n",
    "               s=40, label='Winter <15%', color='blue', edgecolor='black')\n",
    "\n",
    "plt.xlabel('Date')\n",
    "plt.ylabel('Cloud Cover (%)')\n",
    "plt.title('Search Results Timeline')\n",
    "plt.legend()\n",
    "plt.grid(True, alpha=0.3)\n",
    "plt.xticks(rotation=45)\n",
    "\n",
    "# Cloud cover distribution comparison\n",
