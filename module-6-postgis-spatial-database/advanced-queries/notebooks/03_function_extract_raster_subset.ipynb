   "source": [
    "import rasterio\n",
    "import rasterio.mask\n",
    "import rasterio.windows\n",
    "from rasterio.enums import Resampling\n",
    "import numpy as np\n",
    "import geopandas as gpd\n",
    "from shapely.geometry import Point, Polygon, box\n",
    "import os\n",
    "import tempfile\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "def extract_raster_subset(raster_path, bounds=None, geometry=None, \n",
    "                         output_path=None, buffer_distance=0, \n",
    "                         resampling_method='nearest'):\n",
    "    \"\"\"\n",
    "    Extract a spatial subset from a raster dataset.\n",
    "    \"\"\"\n",
    "    \n",
    "    print(f\"üó∫Ô∏è Starting raster subset extraction...\")\n",
    "    print(f\"   üìÅ Input raster: {os.path.basename(raster_path)}\")\n",
    "    \n",
    "    # Step 1: Input validation\n",
    "    if not os.path.exists(raster_path):\n",
    "        raise FileNotFoundError(f\"Raster file not found: {raster_path}\")\n",
    "        \n",
    "    if bounds is None and geometry is None:\n",
    "        raise ValueError(\"Either bounds or geometry must be provided\")\n",
    "        \n",
    "    if bounds is not None and len(bounds) != 4:\n",
    "        raise ValueError(\"Bounds must be a tuple of 4 values (min_x, min_y, max_x, max_y)\")\n",
    "    \n",
    "    # Step 2: Open raster and get metadata\n",
    "    with rasterio.open(raster_path) as src:\n",
    "        src_crs = src.crs\n",
    "        src_bounds = src.bounds\n",
    "        src_shape = src.shape\n",
    "        src_profile = src.profile.copy()\n",
    "        \n",
    "        print(f\"   üìä Source raster info:\")\n",
    "        print(f\"     Shape: {src_shape} (height x width)\")\n",
    "        print(f\"     Bands: {src.count}\")\n",
    "        print(f\"     CRS: {src_crs}\")\n",
    "        print(f\"     Bounds: {src_bounds}\")\n",
    "        \n",
    "        # Step 3: Handle geometry input and CRS alignment\n",
    "        if geometry is not None:\n",
    "            # Handle GeoDataFrame input\n",
    "            if isinstance(geometry, gpd.GeoDataFrame):\n",
    "                if len(geometry) == 0:\n",
    "                    raise ValueError(\"Empty GeoDataFrame provided\")\n",
    "                \n",
    "                # Use first geometry or union of all geometries\n",
    "                if len(geometry) == 1:\n",
    "                    geom = geometry.iloc[0].geometry\n",
    "                    geom_crs = geometry.crs\n",
    "                else:\n",
    "                    print(f\"   üîó Multiple geometries found, creating union\")\n",
    "                    geom = geometry.geometry.unary_union\n",
    "                    geom_crs = geometry.crs\n",
    "            else:\n",
    "                # Assume shapely geometry in same CRS as raster\n",
    "                geom = geometry\n",
    "                geom_crs = src_crs\n",
    "            \n",
    "            # Reproject geometry if needed\n",
    "            if geom_crs != src_crs:\n",
    "                print(f\"   üîÑ Reprojecting geometry from {geom_crs} to {src_crs}\")\n",
    "                temp_gdf = gpd.GeoDataFrame([1], geometry=[geom], crs=geom_crs)\n",
    "                reprojected_gdf = temp_gdf.to_crs(src_crs)\n",
    "                geom = reprojected_gdf.iloc[0].geometry\n",
    "            \n",
    "            # Add buffer if specified\n",
    "            if buffer_distance > 0:\n",
    "                print(f\"   üìè Applying buffer: {buffer_distance} units\")\n",
    "                geom = geom.buffer(buffer_distance)\n",
    "            \n",
    "            # Get bounds from geometry\n",
    "            extract_bounds = geom.bounds\n",
    "            \n",
    "        else:\n",
    "            # Use provided bounds\n",
    "            extract_bounds = bounds\n",
    "            \n",
    "            # Add buffer to bounds if specified\n",
    "            if buffer_distance > 0:\n",
    "                print(f\"   üìè Applying buffer: {buffer_distance} units\")\n",
    "                extract_bounds = (\n",
    "                    bounds[0] - buffer_distance,  # min_x\n",
    "                    bounds[1] - buffer_distance,  # min_y\n",
    "                    bounds[2] + buffer_distance,  # max_x\n",
    "                    bounds[3] + buffer_distance   # max_y\n",
    "                )\n",
    "            \n",
    "            # Create geometry from bounds for masking if needed\n",
    "            geom = box(*extract_bounds)\n",
    "        \n",
    "        print(f\"   üéØ Extraction bounds: {extract_bounds}\")\n",
    "        \n",
    "        # Step 4: Check if bounds intersect with raster\n",
    "        raster_bbox = box(*src_bounds)\n",
    "        extract_bbox = box(*extract_bounds)\n",
    "        \n",
    "        if not raster_bbox.intersects(extract_bbox):\n",
    "            raise ValueError(\"Extraction bounds do not intersect with raster bounds\")\n",
    "        \n",
    "        # Step 5: Calculate extraction window\n",
    "        try:\n",
    "            window = rasterio.windows.from_bounds(*extract_bounds, src.transform)\n",
    "            \n",
    "            # Ensure window is within raster bounds\n",
    "            window = window.intersection(\n",
    "                rasterio.windows.Window(0, 0, src.width, src.height)\n",
    "            )\n",
    "            \n",
    "            if window.width <= 0 or window.height <= 0:\n",
    "                raise ValueError(\"Extraction window has no valid area\")\n",
    "                \n",
    "        except Exception as e:\n",
    "            raise ValueError(f\"Could not calculate extraction window: {e}\")\n",
    "        \n",
    "        print(f\"   üìê Extraction window: {window}\")\n",
    "        print(f\"   üìä Output shape: {int(window.height)} x {int(window.width)}\")\n",
    "        \n",
    "        # Step 6: Extract data\n",
    "        if geometry is not None and hasattr(geometry, 'geometry'):\n",
    "            # Use mask for geometry-based extraction\n",
    "            print(f\"   ‚úÇÔ∏è Extracting and masking with geometry...\")\n",
    "            try:\n",
    "                extracted_data, extracted_transform = rasterio.mask.mask(\n",
    "                    src, [geom], crop=True, nodata=src.nodata\n",
    "                )\n",
    "            except Exception as e:\n",
    "                print(f\"   ‚ö†Ô∏è Geometry masking failed, using window extraction: {e}\")\n",
    "                extracted_data = src.read(window=window)\n",
    "                extracted_transform = rasterio.windows.transform(window, src.transform)\n",
    "        else:\n",
    "            # Use window for bounds-based extraction\n",
    "            print(f\"   ‚úÇÔ∏è Extracting by window...\")\n",
    "            extracted_data = src.read(window=window)\n",
    "            extracted_transform = rasterio.windows.transform(window, src.transform)\n",
    "        \n",
    "        # Step 7: Update profile for extracted data\n",
    "        extracted_profile = src_profile.copy()\n",
    "        extracted_profile.update({\n",
    "            'height': extracted_data.shape[1],\n",
    "            'width': extracted_data.shape[2],\n",
    "            'transform': extracted_transform\n",
    "        })\n",
    "        \n",
    "        # Step 8: Calculate extraction statistics\n",
    "        valid_data = extracted_data[extracted_data != src.nodata] if src.nodata is not None else extracted_data\n",
    "        \n",
    "        extraction_stats = {\n",
    "            'original_shape': src_shape,\n",
    "            'extracted_shape': (extracted_data.shape[1], extracted_data.shape[2]),\n",
    "            'extraction_ratio': {\n",
    "                'width': extracted_data.shape[2] / src_shape[1],\n",
    "                'height': extracted_data.shape[1] / src_shape[0],\n",
    "                'area': (extracted_data.shape[1] * extracted_data.shape[2]) / (src_shape[0] * src_shape[1])\n",
    "            },\n",
    "            'bounds': {\n",
    "                'original': src_bounds,\n",
    "                'extracted': rasterio.transform.array_bounds(\n",
    "                    extracted_data.shape[1], extracted_data.shape[2], extracted_transform\n",
    "                )\n",
    "            },\n",
    "            'data_stats': {\n",
    "                'min': float(np.min(valid_data)) if len(valid_data) > 0 else None,\n",
    "                'max': float(np.max(valid_data)) if len(valid_data) > 0 else None,\n",
    "                'mean': float(np.mean(valid_data)) if len(valid_data) > 0 else None,\n",
    "                'std': float(np.std(valid_data)) if len(valid_data) > 0 else None,\n",
    "                'valid_pixels': int(len(valid_data)),\n",
    "                'total_pixels': int(extracted_data.size),\n",
    "                'valid_percentage': (len(valid_data) / extracted_data.size) * 100 if extracted_data.size > 0 else 0\n",
    "            }\n",
    "        }\n",
    "        \n",
    "        # Step 9: Save to file if output path provided\n",
    "        if output_path:\n",
    "            print(f\"   üíæ Saving extracted raster to: {output_path}\")\n",
    "            \n",
    "            # Ensure output directory exists\n",
    "            os.makedirs(os.path.dirname(output_path), exist_ok=True)\n",
    "            \n",
    "            with rasterio.open(output_path, 'w', **extracted_profile) as dst:\n",
    "                dst.write(extracted_data)\n",
    "                \n",
    "            extraction_stats['output_path'] = output_path\n",
    "            extraction_stats['output_size_mb'] = os.path.getsize(output_path) / (1024 * 1024)\n",
    "        \n",
    "        # Step 10: Compile results\n",
    "        results = {\n",
    "            'extraction_successful': True,\n",
    "            'data': extracted_data,\n",
    "            'profile': extracted_profile,\n",
    "            'transform': extracted_transform,\n",
    "            'window': window,\n",
    "            'bounds': extraction_stats['bounds']['extracted'],\n",
    "            'statistics': extraction_stats,\n",
    "            'extraction_parameters': {\n",
    "                'input_path': raster_path,\n",
    "                'bounds': bounds,\n",
    "                'geometry_provided': geometry is not None,\n",
    "                'buffer_distance': buffer_distance,\n",
    "                'output_path': output_path\n",
    "            }\n",
    "        }\n",
    "        \n",
    "        # Print summary\n",
    "        print(f\"\\nüéâ Raster extraction complete!\")\n",
    "        print(f\"   üìä Extracted shape: {extracted_data.shape[1]} x {extracted_data.shape[2]} pixels\")\n",
    "        print(f\"   üìè Area ratio: {extraction_stats['extraction_ratio']['area']:.2%} of original\")\n",
    "        print(f\"   ‚úÖ Valid data: {extraction_stats['data_stats']['valid_percentage']:.1f}%\")\n",
    "        if output_path:\n",
    "            print(f\"   üíæ File size: {extraction_stats['output_size_mb']:.2f} MB\")\n",
    "        \n",
    "        return results"
