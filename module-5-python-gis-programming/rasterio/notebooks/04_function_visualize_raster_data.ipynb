   "source": [
    "# Demonstrate advanced colormap techniques\n",
    "with rasterio.open(test_datasets['elevation']) as src:\n",
    "    elev_data = src.read(1).astype(float)\n",
    "    extent = [src.bounds.left, src.bounds.right, src.bounds.bottom, src.bounds.top]\n",
    "    elev_data[elev_data == src.nodata] = np.nan\n",
    "\n",
    "fig, axes = plt.subplots(2, 3, figsize=(18, 12))\n",
    "\n",
    "# 1. Standard colormap\n",
    "im1 = axes[0, 0].imshow(elev_data, extent=extent, cmap='terrain', origin='upper')\n",
    "axes[0, 0].set_title('Standard Terrain Colormap')\n",
    "plt.colorbar(im1, ax=axes[0, 0], label='Elevation (m)')\n",
    "\n",
    "# 2. Custom discrete colormap (elevation bands)\n",
    "from matplotlib.colors import BoundaryNorm\n",
    "elevation_levels = [0, 300, 600, 900, 1200, 1500, 1800]\n",
    "colors = ['lightblue', 'green', 'yellow', 'orange', 'brown', 'white']\n",
    "cmap_discrete = mcolors.ListedColormap(colors)\n",
    "norm_discrete = BoundaryNorm(elevation_levels, cmap_discrete.N)\n",
    "\n",
    "im2 = axes[0, 1].imshow(elev_data, extent=extent, cmap=cmap_discrete, \n",
    "                       norm=norm_discrete, origin='upper')\n",
    "axes[0, 1].set_title('Discrete Elevation Bands')\n",
    "cbar2 = plt.colorbar(im2, ax=axes[0, 1], label='Elevation (m)', \n",
    "                     boundaries=elevation_levels, ticks=elevation_levels)\n",
    "\n",
    "# 3. Custom continuous colormap with specific breaks\n",
    "from matplotlib.colors import LinearSegmentedColormap\n",
    "colors_custom = ['blue', 'lightblue', 'green', 'yellow', 'red', 'white']\n",
    "n_bins = 100\n",
    "cmap_custom = LinearSegmentedColormap.from_list('custom_terrain', colors_custom, N=n_bins)\n",
    "cmap_custom.set_bad(color='gray', alpha=0.3)\n",
    "\n",
    "im3 = axes[0, 2].imshow(elev_data, extent=extent, cmap=cmap_custom, origin='upper')\n",
    "axes[0, 2].set_title('Custom Continuous Colormap')\n",
    "plt.colorbar(im3, ax=axes[0, 2], label='Elevation (m)')\n",
    "\n",
    "# 4. Hillshade effect (simulated)\n",
    "from scipy import ndimage\n",
    "# Create hillshade effect using gradients\n",
    "grad_x = ndimage.sobel(elev_data, axis=1)\n",
    "grad_y = ndimage.sobel(elev_data, axis=0)\n",
    "hillshade = np.sqrt(grad_x**2 + grad_y**2)\n",
    "hillshade = 255 - hillshade  # Invert for shading effect\n",
    "\n",
    "im4 = axes[1, 0].imshow(elev_data, extent=extent, cmap='terrain', origin='upper', alpha=0.8)\n",
    "axes[1, 0].imshow(hillshade, extent=extent, cmap='gray', origin='upper', alpha=0.3)\n",
    "axes[1, 0].set_title('Elevation with Hillshade')\n",
    "plt.colorbar(im4, ax=axes[1, 0], label='Elevation (m)')\n",
    "\n",
    "# 5. Stretched colormap (enhance contrast)\n",
    "# Use percentile-based stretching\n",
    "valid_data = elev_data[~np.isnan(elev_data)]\n",
    "p2, p98 = np.percentile(valid_data, [2, 98])\n",
    "\n",
    "im5 = axes[1, 1].imshow(elev_data, extent=extent, cmap='terrain', \n",
    "                       origin='upper', vmin=p2, vmax=p98)\n",
    "axes[1, 1].set_title(f'Contrast Stretched (2%-98%)')\n",
    "plt.colorbar(im5, ax=axes[1, 1], label='Elevation (m)')\n",
    "\n",
    "# 6. Bivariate visualization (elevation + slope)\n",
    "# Calculate slope\n",
    "slope = np.sqrt(grad_x**2 + grad_y**2)\n",
    "slope[np.isnan(elev_data)] = np.nan\n",
    "\n",
    "im6 = axes[1, 2].imshow(slope, extent=extent, cmap='Reds', origin='upper')\n",
    "axes[1, 2].set_title('Slope (derived from elevation)')\n",
    "plt.colorbar(im6, ax=axes[1, 2], label='Slope')\n",
    "\n",
    "# Add coordinate labels\n",
    "for i in range(2):\n",
    "    for j in range(3):\n",
    "        if i == 1:  # Bottom row\n",
    "            axes[i, j].set_xlabel('Longitude')\n",
    "        if j == 0:  # Left column\n",
    "            axes[i, j].set_ylabel('Latitude')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(\"=== ADVANCED COLORMAP TECHNIQUES ===\")\n",
    "print(\"1. Discrete bands: Useful for categorical or threshold-based data\")\n",
    "print(\"2. Custom continuous: Brand colors or specific visual requirements\")\n",
    "print(\"3. Hillshade overlay: Adds 3D appearance to elevation data\")\n",
    "print(\"4. Contrast stretching: Enhances subtle variations in data\")\n",
    "print(\"5. Derived products: Show related information (slope from elevation)\")"
