    "plt.axvline(x=30, color='darkred', linestyle='--', alpha=0.7, label='Very steep threshold')\n",
    "plt.xlabel('Slope (degrees)')\n",
    "plt.ylabel('Frequency')\n",
    "plt.title('Slope Distribution')\n",
