   "cell_type": "code",
   "execution_count": null,
   "id": "spatial-aggregation-function",
   "metadata": {},
   "outputs": [],
   "source": [
    "def aggregate_by_spatial_join(points_gdf, polygons_gdf, agg_column, agg_functions=['count', 'sum']):\n",
    "    \"\"\"\n",
    "    Aggregate point data by polygon areas using spatial joins.\n",
    "    \n",
    "    This is another important function for spatial_joins_analysis.py!\n",
    "    \n",
    "    Parameters:\n",
    "    -----------\n",
    "    points_gdf : GeoDataFrame\n",
    "        Points to aggregate\n",
    "    polygons_gdf : GeoDataFrame\n",
    "        Polygon boundaries for aggregation\n",
    "    agg_column : str\n",
    "        Column to aggregate\n",
    "    agg_functions : list\n",
    "        List of aggregation functions ('count', 'sum', 'mean', 'min', 'max')\n",
    "        \n",
    "    Returns:\n",
    "    --------\n",
    "    GeoDataFrame : Polygons with aggregated statistics\n",
    "    \"\"\"\n",
    "    # Step 1: Perform spatial join\n",
    "    joined = perform_spatial_join(points_gdf, polygons_gdf, how='inner', predicate='within')\n",
    "    \n",
    "    # Step 2: Prepare aggregation dictionary\n",
    "    agg_dict = {}\n",
    "    for func in agg_functions:\n",
    "        if func == 'count':\n",
    "            agg_dict[f'{agg_column}_{func}'] = (agg_column, 'count')\n",
    "        elif func in ['sum', 'mean', 'min', 'max']:\n",
    "            agg_dict[f'{agg_column}_{func}'] = (agg_column, func)\n",
    "    \n",
    "    # Step 3: Group by polygon index and aggregate\n",
    "    # Use index_right which corresponds to the polygon that contains each point\n",
    "    aggregated = joined.groupby('index_right').agg(agg_dict).reset_index()\n",
    "    \n",
    "    # Step 4: Merge back with original polygons\n",
    "    result = polygons_gdf.copy()\n",
    "    result = result.reset_index().rename(columns={'index': 'index_right'})\n",
    "    result = result.merge(aggregated, on='index_right', how='left')\n",
    "    \n",
    "    # Step 5: Fill NaN values with 0 for areas with no points\n",
    "    for col in agg_dict.keys():\n",
    "        result[col] = result[col].fillna(0)\n",
    "    \n",
    "    return result\n",
    "\n",
    "# Test: Aggregate city population by county\n",
    "county_population = aggregate_by_spatial_join(\n",
    "    cities_gdf, counties_gdf, 'population', ['count', 'sum', 'mean']\n",
    ")\n",
    "\n",
    "print(\"County Population Statistics (from cities):\")\n",
    "for idx, row in county_population.iterrows():\n",
    "    print(f\"{row['name']} County:\")\n",
    "    print(f\"  Cities: {int(row['population_count'])}\")\n",
    "    print(f\"  Total Population: {int(row['population_sum']):,}\")\n",
    "    print(f\"  Average City Size: {int(row['population_mean']):,}\")\n",
    "    print()"
   ]
