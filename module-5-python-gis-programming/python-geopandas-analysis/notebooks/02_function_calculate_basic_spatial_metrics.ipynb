    "polygon2 = Polygon([(-118.3, 34.0), (-118.2, 34.0), (-118.2, 34.1), (-118.3, 34.1), (-118.3, 34.0)])\n",
    "\n",
    "# Create some sample lines (representing rivers or roads)\n",
    "line1 = LineString([(-122.5, 37.75), (-122.45, 37.76), (-122.4, 37.77)])\n",
