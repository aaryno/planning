    "right_joined = pd.merge(readings_df, stations_df, on='station_id', how='right')\n",
    "\n",
    "print(f\"📊 Original stations: {len(stations_df)}\")\n",
    "print(f\"📊 After right join: {len(right_joined)} rows\")\n",
