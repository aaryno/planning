    "def create_spatial_buffer_analysis(gdf: gpd.GeoDataFrame, buffer_distance: float) -> dict:\n",
    "    \"\"\"\n",
    "    Create buffer zones around spatial features and perform proximity analysis.\n",
    "    \n",
    "    Args:\n",
    "        gdf (gpd.GeoDataFrame): Input spatial features\n",
    "        buffer_distance (float): Buffer distance in meters\n",
    "        \n",
    "    Returns:\n",
    "        Dict[str, Any]: Dictionary with buffer analysis results\n",
    "    \"\"\"\n",
    "    \n",
    "    print(\"\\n\" + \"=\" * 60)\n",
    "    print(\"CREATE SPATIAL BUFFER ANALYSIS\")\n",
    "    print(\"=\" * 60)\n",
    "    \n",
    "    # Step 1: Input validation\n",
    "    if gdf is None or gdf.empty:\n",
    "        print(\"❌ ERROR: Input GeoDataFrame is None or empty\")\n",
    "        return None\n",
    "    \n",
    "    if buffer_distance <= 0:\n",
    "        print(\"❌ ERROR: Buffer distance must be positive\")\n",
    "        return None\n",
    "    \n",
    "    print(f\"📊 Creating {buffer_distance}m buffers around {len(gdf)} features\")\n",
    "    geometry_counts = gdf.geom_type.value_counts()\n",
    "    print(f\"🔺 Input geometry types: {geometry_counts.to_dict()}\")\n",
    "    \n",
    "    # Step 2: Handle coordinate reference system\n",
    "    original_crs = gdf.crs\n",
    "    print(f\"\\n🌍 Original CRS: {original_crs}\")\n",
    "    \n",
    "    if original_crs is None:\n",
    "        print(\"⚠️  Warning: No CRS defined. Assuming EPSG:4326\")\n",
    "        gdf = gdf.set_crs('EPSG:4326')\n",
    "        original_crs = gdf.crs\n",
    "    \n",
    "    # Check if we need projected coordinates for accurate buffering\n",
    "    needs_projection = 'EPSG:4326' in str(original_crs) or original_crs.is_geographic\n",
    "    \n",
    "    if needs_projection:\n",
    "        # Determine appropriate UTM zone\n",
    "        bounds = gdf.total_bounds\n",
    "        center_lon = (bounds[0] + bounds[2]) / 2\n",
    "        center_lat = (bounds[1] + bounds[3]) / 2\n",
    "        \n",
    "        utm_zone = int((center_lon + 180) / 6) + 1\n",
    "        hemisphere = 'N' if center_lat >= 0 else 'S'\n",
    "        \n",
    "        if hemisphere == 'N':\n",
    "            utm_crs = f\"EPSG:326{utm_zone:02d}\"\n",
    "        else:\n",
    "            utm_crs = f\"EPSG:327{utm_zone:02d}\"\n",
    "        \n",
    "        print(f\"📐 Reprojecting to {utm_crs} for accurate buffering\")\n",
    "        gdf_buffer = gdf.to_crs(utm_crs)\n",
    "        buffer_crs = utm_crs\n",
    "    else:\n",
    "        print(\"📐 Using original projected coordinates for buffering\")\n",
    "        gdf_buffer = gdf.copy()\n",
    "        buffer_crs = original_crs\n",
    "    \n",
    "    # Step 3: Create buffer zones\n",
    "    print(f\"\\n🔄 Creating buffer zones...\")\n",
    "    buffer_geometries = gdf_buffer.buffer(buffer_distance)\n",
    "    \n",
    "    # Create buffered GeoDataFrame\n",
    "    buffered_gdf = gpd.GeoDataFrame(\n",
    "        gdf_buffer.drop('geometry', axis=1),\n",
    "        geometry=buffer_geometries,\n",
    "        crs=buffer_crs\n",
    "    )\n",
    "    \n",
    "    print(f\"✅ Created {len(buffered_gdf)} buffer zones\")\n",
    "    \n",
    "    # Step 4: Calculate buffer areas\n",
    "    print(f\"\\n📏 Calculating buffer areas...\")\n",
    "    buffer_areas_m2 = buffered_gdf.area\n",
    "    buffer_areas_km2 = buffer_areas_m2 / 1_000_000\n",
    "    \n",
    total_buffer_area_km2 = buffer_areas_km2.sum()
    average_buffer_area_km2 = buffer_areas_km2.mean()

    # Step 5: Detect overlapping buffers
    print(f"🔍 Analyzing buffer overlaps...")

    # Create union of all buffers
    merged_buffers = unary_union(buffered_gdf.geometry)
    merged_area_m2 = merged_buffers.area
    merged_area_km2 = merged_area_m2 / 1_000_000

    # Calculate overlap statistics
    overlap_area_km2 = total_buffer_area_km2 - merged_area_km2

    # Count overlapping pairs
    overlap_count = 0
    for i in range(len(buffered_gdf)):
        for j in range(i+1, len(buffered_gdf)):
            if buffered_gdf.iloc[i].geometry.intersects(buffered_gdf.iloc[j].geometry):
                overlap_count += 1

    # Step 6: Display results summary
    print(f"\n📊 BUFFER ANALYSIS RESULTS:")
    print(f"   Buffers created: {len(buffered_gdf)}")
    print(f"   Buffer distance: {buffer_distance} meters")
    print(f"   Total buffered area: {total_buffer_area_km2:.6f} km²")
    print(f"   Average buffer area: {average_buffer_area_km2:.6f} km²")
    print(f"   Actual coverage area: {merged_area_km2:.6f} km²")
    print(f"   Overlapping buffer pairs: {overlap_count}")
    print(f"   CRS used for calculations: {buffer_crs}")

    # Step 7: Convert back to original CRS for output
    buffered_gdf_output = buffered_gdf.to_crs(original_crs)
    original_gdf_output = gdf_buffer.to_crs(original_crs)

    # Step 8: Return comprehensive results dictionary
    results = {
        'buffered_gdf': buffered_gdf_output,
        'original_gdf': original_gdf_output,
        'buffer_areas': buffer_areas_km2,
        'total_buffered_area': total_buffer_area_km2,
        'average_buffer_area': average_buffer_area_km2,
        'actual_coverage_area': merged_area_km2,
        'overlapping_buffers': overlap_count,
        'distance_used': buffer_distance,
        'crs_used': buffer_crs
    }

    print(f"\n🎉 Buffer analysis complete!")
    return results
