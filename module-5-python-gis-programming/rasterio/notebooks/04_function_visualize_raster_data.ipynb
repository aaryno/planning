    "# Create a professional-quality raster visualization\n",
    "def create_professional_raster_plot(raster_path, title=None, colormap='viridis', \n",
    "                                   figsize=(10, 8), save_path=None):\n",
    "    \"\"\"Create a complete professional raster visualization.\"\"\"\n",
    "    \n",
    "    with rasterio.open(raster_path) as src:\n",
    "        data = src.read(1).astype(float)\n",
    "        bounds = src.bounds\n",
    "        nodata = src.nodata\n",
    "        crs = src.crs\n",
    "        \n",
    "        # Handle NoData\n",
    "        if nodata is not None:\n",
    "            data[data == nodata] = np.nan\n",
    "        \n",
    "        # Set up the plot\n",
    "        fig, ax = plt.subplots(figsize=figsize)\n",
    "        \n",
    "        # Create the main raster plot\n",
    "        extent = [bounds.left, bounds.right, bounds.bottom, bounds.top]\n",
    "        \n",
    "        # Custom colormap handling\n",
    "        if isinstance(colormap, str):\n",
    "            cmap = plt.cm.get_cmap(colormap)\n",
    "        else:\n",
    "            cmap = colormap\n",
    "        \n",
    "        # Set NoData color\n",
    "        cmap.set_bad(color='lightgray', alpha=0.5)\n",
    "        \n",
    "        # Main image\n",
    "        im = ax.imshow(data, extent=extent, cmap=cmap, origin='upper')\n",
    "        \n",
    "        # Colorbar with proper labeling\n",
    "        cbar = plt.colorbar(im, ax=ax, shrink=0.8, aspect=20)\n",
    "        \n",
    "        # Determine appropriate units from filename or data\n",
    "        if 'elevation' in raster_path.lower():\n",
    "            cbar.set_label('Elevation (m)', fontsize=12)\n",
    "        elif 'temperature' in raster_path.lower():\n",
    "            cbar.set_label('Temperature (Â°C)', fontsize=12)\n",
    "        elif 'vegetation' in raster_path.lower():\n",
    "            cbar.set_label('Vegetation Index', fontsize=12)\n",
    "        else:\n",
