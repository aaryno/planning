    "   \"id\": \"interactive-mapping-section\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"## üåç Part 4: Interactive Maps with Folium\\n\",\n",
    "    \"\\n\",\n",
    "    \"Folium creates interactive maps that users can pan, zoom, and click on.\\n\",\n",
    "    \"\\n\",\n",
    "    \"**Perfect for:**\\n\",\n",
    "    \"- Web-based dashboards\\n\",\n",
    "    \"- Exploratory data analysis\\n\",\n",
    "    \"- Presentations and reports\\n\",\n",
    "    \"- Sharing maps with non-GIS users\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"id\": \"interactive-map-function\",\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"def create_interactive_map(gdf, popup_columns=None, center=None, zoom_start=8, \\n\",\n",
    "    \"                          tile_layer='OpenStreetMap', marker_color='red'):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    Create an interactive map using Folium.\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    This is the final key function for visualization_mapping.py!\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    Parameters:\\n\",\n",
    "    \"    -----------\\n\",\n",
    "    \"    gdf : GeoDataFrame\\n\",\n",
    "    \"        GeoDataFrame to visualize\\n\",\n",
    "    \"    popup_columns : list, optional\\n\",\n",
    "    \"        Columns to show in popups when features are clicked\\n\",\n",
    "    \"    center : tuple, optional\\n\",\n",
    "    \"        Center coordinates (lat, lon). If None, uses data extent\\n\",\n",
    "    \"    zoom_start : int\\n\",\n",
    "    \"        Initial zoom level\\n\",\n",
    "    \"    tile_layer : str\\n\",\n",
    "    \"        Base tile layer ('OpenStreetMap', 'CartoDB positron', etc.)\\n\",\n",
    "    \"    marker_color : str\\n\",\n",
    "    \"        Color for point markers\\n\",\n",
    "    \"        \\n\",\n",
    "    \"    Returns:\\n\",\n",
    "    \"    --------\\n\",\n",
    "    \"    folium.Map : Interactive Folium map object\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    # Step 1: Ensure GeoDataFrame is in WGS84 for Folium\\n\",\n",
    "    \"    if gdf.crs != 'EPSG:4326':\\n\",\n",
    "    \"        print(f'‚ö†Ô∏è Reprojecting from {gdf.crs} to EPSG:4326 for web mapping...')\\n\",\n",
    "    \"        gdf = gdf.to_crs('EPSG:4326')\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Step 2: Determine map center\\n\",\n",
    "    \"    if center is None:\\n\",\n",
    "    \"        bounds = gdf.total_bounds  # [minx, miny, maxx, maxy]\\n\",\n",
    "    \"        center_lat = (bounds[1] + bounds[3]) / 2\\n\",\n",
    "    \"        center_lon = (bounds[0] + bounds[2]) / 2\\n\",\n",
    "    \"        center = [center_lat, center_lon]\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Step 3: Create base map\\n\",\n",
    "    \"    m = folium.Map(location=center, zoom_start=zoom_start, tiles=tile_layer)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Step 4: Add features based on geometry type\\n\",\n",
    "    \"    for idx, row in gdf.iterrows():\\n\",\n",
    "    \"        geom = row.geometry\\n\",\n",
    "    \"        \\n\",\n",
    "    \"        # Create popup content\\n\",\n",
    "    \"        popup_content = f'<b>Feature {idx}</b><br>'\\n\",\n",
    "    \"        \\n\",\n",
    "    \"        if popup_columns:\\n\",\n",
    "    \"            for col in popup_columns:\\n\",\n",
    "    \"                if col in row.index and pd.notna(row[col]):\\n\",\n",
    "    \"                    popup_content += f'<b>{col}:</b> {row[col]}<br>'\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            # Show all non-geometry columns\\n\",\n",
    "    \"            for col in gdf.columns:\\n\",\n",
    "    \"                if col != 'geometry' and pd.notna(row[col]):\\n\",\n",
    "    \"                    popup_content += f'<b>{col}:</b> {row[col]}<br>'\\n\",\n",
    "    \"        \\n\",\n",
    "    \"        popup = folium.Popup(popup_content, max_width=300)\\n\",\n",
    "    \"        \\n\",\n",
    "    \"        # Add geometry to map\\n\",\n",
    "    \"        if geom.geom_type == 'Point':\\n\",\n",
    "    \"            folium.Marker(\\n\",\n",
    "    \"                location=[geom.y, geom.x],\\n\",\n",
    "    \"                popup=popup,\\n\",\n",
    "    \"                icon=folium.Icon(color=marker_color)\\n\",\n",
    "    \"            ).add_to(m)\\n\",\n",
    "    \"        elif geom.geom_type in ['Polygon', 'MultiPolygon']:\\n\",\n",
    "    \"            # Convert to GeoJSON for folium\\n\",\n",
    "    \"            geojson = gpd.GeoSeries([geom]).to_json()\\n\",\n",
    "    \"            folium.GeoJson(\\n\",\n",
    "    \"                geojson,\\n\",\n",
    "    \"                popup=popup,\\n\",\n",
    "    \"                style_function=lambda x: {\\n\",\n",
    "    \"                    'fillColor': 'lightblue',\\n\",\n",
    "    \"                    'color': 'black',\\n\",\n",
    "    \"                    'weight': 2,\\n\",\n",
    "    \"                    'fillOpacity': 0.6\\n\",\n",
    "    \"                }\\n\",\n",
    "    \"            ).add_to(m)\\n\",\n",
    "    \"        elif geom.geom_type in ['LineString', 'MultiLineString']:\\n\",\n",
    "    \"            # Convert to coordinate list\\n\",\n",
    "    \"            if geom.geom_type == 'LineString':\\n\",\n",
    "    \"                coords = [[point[1], point[0]] for point in geom.coords]\\n\",\n",
    "    \"            else:\\n\",\n",
    "    \"                coords = []\\n\",\n",
    "    \"                for line in geom.geoms:\\n\",\n",
    "    \"                    coords.extend([[point[1], point[0]] for point in line.coords])\\n\",\n",
    "    \"            \\n\",\n",
    "    \"            folium.PolyLine(\\n\",\n",
    "    \"                coords,\\n\",\n",
    "    \"                popup=popup,\\n\",\n",
    "    \"                color='red',\\n\",\n",
    "    \"                weight=3,\\n\",\n",
    "    \"                opacity=0.8\\n\",\n",
    "    \"            ).add_to(m)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Step 5: Fit map to data bounds\\n\",\n",
    "    \"    if len(gdf) > 0:\\n\",\n",
    "    \"        bounds = gdf.total_bounds\\n\",\n",
    "    \"        m.fit_bounds([[bounds[1], bounds[0]], [bounds[3], bounds[2]]])\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    return m\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Test interactive mapping\\n\",\n",
    "    \"cities_map = create_interactive_map(\\n\",\n",
    "    \"    cities_gdf, \\n\",\n",
    "    \"    popup_columns=['name', 'population', 'type', 'quality_of_life'],\\n\",\n",
    "    \"    marker_color='blue'\\n\",\n",
    "    \")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Display the map\\n\",\n",
    "    \"cities_map\"\n",
    "   ]\n",
    "  },\n",
