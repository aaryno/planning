    "## ðŸ“ Step 5: Creating Professional Plot Layout\n",
    "\n",
    "Let's build a complete, professional visualization with all the elements:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def create_professional_raster_plot(raster_path, band_number=1, title=None):\n",
    "    \"\"\"\n",
    "    Create a professional raster visualization.\n",
    "    This demonstrates all the elements your function should include.\n",
    "    \"\"\"\n",
    "    with rasterio.open(raster_path) as src:\n",
    "        # Read the data\n",
    "        band_data = src.read(band_number)\n",
    "        \n",
    "        # Handle NoData\n",
    "        if src.nodata is not None:\n",
    "            display_data = np.ma.masked_equal(band_data, src.nodata)\n",
    "        else:\n",
    "            display_data = band_data\n",
    "        \n",
    "        # Create figure with good size\n",
    "        fig, ax = plt.subplots(figsize=(12, 10))\n",
    "        \n",
    "        # Choose appropriate colormap based on data type\n",
    "        filename_lower = Path(raster_path).name.lower()\n",
    "        if 'elevation' in filename_lower or 'dem' in filename_lower:\n",
    "            cmap = 'terrain'\n",
    "            label = 'Elevation (m)'\n",
    "        elif 'temp' in filename_lower:\n",
    "            cmap = 'coolwarm'\n",
    "            label = 'Temperature'\n",
    "        elif 'ndvi' in filename_lower:\n",
    "            cmap = 'RdYlGn'\n",
    "            label = 'NDVI'\n",
    "        else:\n",
    "            cmap = 'viridis'\n",
    "            label = 'Value'\n",
    "        \n",
    "        # Create the main plot with geographic extent\n",
    "        im = ax.imshow(display_data, \n",
    "                      cmap=cmap,\n",
    "                      extent=[src.bounds.left, src.bounds.right, \n",
    "                             src.bounds.bottom, src.bounds.top],\n",
    "                      aspect='equal')\n",
    "        \n",
    "        # Add colorbar with proper label\n",
    "        cbar = plt.colorbar(im, ax=ax, shrink=0.8, pad=0.02)\n",
    "        cbar.set_label(label, fontsize=12)\n",
    "        \n",
    "        # Set title\n",
    "        if title is None:\n",
    "            title = f'Raster Visualization: {Path(raster_path).stem}'\n",
    "            if src.count > 1:\n",
    "                title += f' (Band {band_number})'\n",
    "        \n",
    "        ax.set_title(title, fontsize=14, fontweight='bold', pad=20)\n",
    "        \n",
    "        # Set axis labels\n",
    "        ax.set_xlabel('Longitude', fontsize=12)\n",
    "        ax.set_ylabel('Latitude', fontsize=12)\n",
    "        \n",
    "        # Add grid for reference\n",
    "        ax.grid(True, alpha=0.3, linestyle='--')\n",
    "        \n",
    "        # Add coordinate system info\n",
    "        crs_text = f'CRS: {src.crs}'\n",
    "        ax.text(0.02, 0.02, crs_text, transform=ax.transAxes, \n",
    "               bbox=dict(boxstyle='round', facecolor='white', alpha=0.8),\n",
    "               fontsize=9)\n",
    "        \n",
    "        # Add data summary\n",
    "        if src.nodata is not None:\n",
    "            valid_data = display_data.compressed()  # Get non-masked values\n",
    "        else:\n",
    "            valid_data = display_data.flatten()\n",
    "        \n",
    "        if len(valid_data) > 0:\n",
    "            stats_text = f\"\"\"Data Summary:\n",
    "Min: {np.min(valid_data):.1f}\n",
    "Max: {np.max(valid_data):.1f}\n",
    "Mean: {np.mean(valid_data):.1f}\n",
    "Pixels: {len(valid_data):,}\"\"\"\n",
    "        else:\n",
    "            stats_text = \"No valid data\"\n",
    "        \n",
    "        ax.text(0.98, 0.02, stats_text, transform=ax.transAxes,\n",
    "               verticalalignment='bottom', horizontalalignment='right',\n",
    "               bbox=dict(boxstyle='round', facecolor='white', alpha=0.8),\n",
    "               fontsize=9)\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        return fig\n",
    "\n",
    "# Test our professional plotting function\n",
    "if elevation_file.exists():\n",
    "    print(\"ðŸŽ¨ Creating professional raster visualization...\")\n",
    "    fig = create_professional_raster_plot(elevation_file)\n",
    "    plt.show()\n",
    "    print(\"âœ… Professional plot created!\")\nelse:\n",
    "    print(\"âŒ Cannot create plot - sample data not available\")"
   ]
  },
