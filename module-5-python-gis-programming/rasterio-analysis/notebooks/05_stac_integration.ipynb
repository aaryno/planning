    "def simulate_stac_search(catalog_data, bbox=None, datetime_range=None, \n",
    "                        collections=None, max_cloud_cover=None, limit=100):\n",
    "    \"\"\"Simulate STAC API search functionality.\"\"\"\n",
    "    \n",
    "    filtered_items = catalog_data['features'].copy()\n",
    "    \n",
    "    # Apply spatial filtering\n",
    "    if bbox:\n",
    "        spatially_filtered = []\n",
    "        search_bbox = box(bbox[0], bbox[1], bbox[2], bbox[3])\n",
    "        \n",
    "        for item in filtered_items:\n",
    "            item_bbox = box(item['bbox'][0], item['bbox'][1], \n",
    "                          item['bbox'][2], item['bbox'][3])\n",
    "            if search_bbox.intersects(item_bbox):\n",
    "                spatially_filtered.append(item)\n",
    "        \n",
    "        filtered_items = spatially_filtered\n",
    "        print(f\"After spatial filtering: {len(filtered_items)} items\")\n",
    "    \n",
    "    # Apply temporal filtering\n",
    "    if datetime_range:\n",
    "        start_date, end_date = datetime_range.split('/')\n",
    "        start_dt = datetime.fromisoformat(start_date.replace('Z', ''))\n",
    "        end_dt = datetime.fromisoformat(end_date.replace('Z', ''))\n",
    "        \n",
    "        temporally_filtered = []\n",
    "        for item in filtered_items:\n",
    "            item_dt = datetime.fromisoformat(item['properties']['datetime'].replace('Z', ''))\n",
    "            if start_dt <= item_dt <= end_dt:\n",
    "                temporally_filtered.append(item)\n",
    "        \n",
    "        filtered_items = temporally_filtered\n",
    "        print(f\"After temporal filtering: {len(filtered_items)} items\")\n",
    "    \n",
    "    # Apply cloud cover filtering\n",
    "    if max_cloud_cover is not None:\n",
    "        cloud_filtered = []\n",
    "        for item in filtered_items:\n",
    "            if item['properties']['eo:cloud_cover'] <= max_cloud_cover:\n",
    "                cloud_filtered.append(item)\n",
    "        \n",
    "        filtered_items = cloud_filtered\n",
    "        print(f\"After cloud cover filtering (≤{max_cloud_cover}%): {len(filtered_items)} items\")\n",
    "    \n",
    "    # Apply limit\n",
    "    if len(filtered_items) > limit:\n",
    "        filtered_items = filtered_items[:limit]\n",
    "        print(f\"Applied limit: {len(filtered_items)} items\")\n",
    "    \n",
    "    return {\n",
    "        \"type\": \"FeatureCollection\",\n",
    "        \"features\": filtered_items,\n",
    "        \"context\": {\n",
    "            \"matched\": len(filtered_items),\n",
    "            \"returned\": len(filtered_items)\n",
    "        }\n",
    "    }\n",
    "\n",
    "def extract_metadata_dataframe(stac_response):\n",
    "    \"\"\"Convert STAC response to pandas DataFrame for analysis.\"\"\"\n",
    "    \n",
    "    records = []\n",
    "    for item in stac_response['features']:\n",
    "        record = {\n",
    "            'id': item['id'],\n",
    "            'datetime': datetime.fromisoformat(item['properties']['datetime'].replace('Z', '')),\n",
    "            'platform': item['properties'].get('platform', 'unknown'),\n",
    "            'cloud_cover': item['properties'].get('eo:cloud_cover', 0),\n",
    "            'path': item['properties'].get('landsat:wrs_path', 0),\n",
    "            'row': item['properties'].get('landsat:wrs_row', 0),\n",
    "            'bbox_area': (item['bbox'][2] - item['bbox'][0]) * (item['bbox'][3] - item['bbox'][1]),\n",
    "            'processing_level': item['properties'].get('processing:level', 'unknown')\n",
    "        }\n",
    "        records.append(record)\n",
    "    \n",
    "    return pd.DataFrame(records)\n",
    "\n",
    "# Demonstrate different search scenarios\n",
    "print(\"\\nTesting STAC Search Scenarios:\")\n",
    "print(\"=\" * 40)\n",
    "\n",
    "# Scenario 1: Summer months with low cloud cover\n",
    "print(\"\\n1. Summer scenes with <20% cloud cover:\")\n",
    "summer_search = simulate_stac_search(\n",
    "    mock_catalog,\n",
    "    datetime_range=\"2023-06-01T00:00:00Z/2023-08-31T23:59:59Z\",\n",
    "    max_cloud_cover=20\n",
    ")\n",
    "\n",
    "# Scenario 2: Spatial subset\n",
    "print(\"\\n2. Spatial subset (central Bay Area):\")\n",
    "spatial_search = simulate_stac_search(\n",
    "    mock_catalog,\n",
    "    bbox=[-122.3, 37.6, -122.0, 37.9],\n",
    "    max_cloud_cover=30\n",
    ")\n",
    "\n",
    "# Scenario 3: High-quality winter scenes\n",
    "print(\"\\n3. Winter scenes with <15% cloud cover:\")\n",
    "winter_search = simulate_stac_search(\n",
    "    mock_catalog,\n",
    "    datetime_range=\"2023-12-01T00:00:00Z/2023-12-31T23:59:59Z\",\n",
    "    max_cloud_cover=15\n",
    ")\n",
    "\n",
    "# Convert results to DataFrames for analysis\n",
    "summer_df = extract_metadata_dataframe(summer_search)\n",
    "spatial_df = extract_metadata_dataframe(spatial_search)\n",
    "winter_df = extract_metadata_dataframe(winter_search)\n",
    "\n",
    "# Visualize search results\n",
    "plt.figure(figsize=(18, 10))\n",
    "\n",
    "# Timeline of all searches\n",
    "plt.subplot(2, 3, 1)\n",
