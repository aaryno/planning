    "# Statistical comparison\n",
    "print(\"\\nVegetation Index Statistics:\")\n",
    "print(f\"NDVI - Range: {np.nanmin(ndvi):.3f} to {np.nanmax(ndvi):.3f}, Mean: {np.nanmean(ndvi):.3f}\")\n",
    "print(f\"EVI  - Range: {np.nanmin(evi):.3f} to {np.nanmax(evi):.3f}, Mean: {np.nanmean(evi):.3f}\")\n",
    "\n",
    "# Land cover analysis\n",
    "forest_mask, agriculture_mask, water_mask, urban_mask = land_cover_masks\n",
    "print(\"\\nNDVI by Land Cover Type:\")\n",
    "print(f\"Forest:      {np.nanmean(ndvi[forest_mask]):.3f} ± {np.nanstd(ndvi[forest_mask]):.3f}\")\n",
    "print(f\"Agriculture: {np.nanmean(ndvi[agriculture_mask]):.3f} ± {np.nanstd(ndvi[agriculture_mask]):.3f}\")\n",
    "print(f\"Water:       {np.nanmean(ndvi[water_mask]):.3f} ± {np.nanstd(ndvi[water_mask]):.3f}\")\n",
    "print(f\"Urban/Bare:  {np.nanmean(ndvi[urban_mask]):.3f} ± {np.nanstd(ndvi[urban_mask]):.3f}\")"
