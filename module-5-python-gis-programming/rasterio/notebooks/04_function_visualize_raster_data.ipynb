    "import rasterio\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import matplotlib.colors as mcolors\n",
    "from matplotlib.patches import Rectangle\n",
    "import seaborn as sns\n",
    "from pathlib import Path\n",
    "import tempfile\n",
    "import os\n",
    "from typing import Dict, Any, Optional, Tuple\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "# Create diverse test datasets for visualization\n",
    "def create_visualization_datasets():\n",
    "    \"\"\"Create multiple raster datasets with different characteristics for visualization testing.\"\"\"\n",
    "    \n",
    "    datasets = {}\n",
    "    width, height = 150, 150\n",
    "    \n",
    "    # Common transform for all datasets\n",
    "    transform = rasterio.transform.from_bounds(\n",
    "        west=-121.5, south=36.5, east=-120.0, north=38.0,\n",
    "        width=width, height=height\n",
    "    )\n",
    "    \n",
    "    # Dataset 1: Elevation data\n",
    "    x = np.linspace(-2, 2, width)\n",
    "    y = np.linspace(-2, 2, height)\n",
    "    X, Y = np.meshgrid(x, y)\n",
    "    \n",
    "    elevation = (500 + \n",
    "                1200 * np.exp(-(X**2 + Y**2)) +  # Main peak\n",
    "                400 * np.exp(-((X-1)**2 + (Y+0.5)**2)*2) +  # Secondary peak\n",
    "                np.random.normal(0, 30, (height, width)))  # Terrain roughness\n",
    "    \n",
    "    # Add water bodies (set to NoData)\n",
    "    water_mask = elevation < 300\n",
    "    elevation[water_mask] = -9999\n",
    "    \n",
    "    elev_path = tempfile.mktemp(suffix='_elevation.tif')\n",
    "    with rasterio.open(\n",
    "        elev_path, 'w', driver='GTiff', height=height, width=width,\n",
    "        count=1, dtype=elevation.dtype, crs='EPSG:4326',\n",
    "        transform=transform, nodata=-9999\n",
    "    ) as dst:\n",
    "        dst.write(elevation, 1)\n",
    "    \n",
    "    datasets['elevation'] = elev_path\n",
    "    \n",
    "    # Dataset 2: Temperature data\n",
    "    temperature = 15 + 10 * np.sin(X * 2) * np.cos(Y * 2) + np.random.normal(0, 2, (height, width))\n",
    "    \n",
    "    # Add some missing temperature readings\n",
    "    temp_missing = np.random.random((height, width)) < 0.05\n",
    "    temperature[temp_missing] = -999\n",
    "    \n",
    "    temp_path = tempfile.mktemp(suffix='_temperature.tif')\n",
    "    with rasterio.open(\n",
    "        temp_path, 'w', driver='GTiff', height=height, width=width,\n",
    "        count=1, dtype=temperature.dtype, crs='EPSG:4326',\n",
    "        transform=transform, nodata=-999\n",
    "    ) as dst:\n",
    "        dst.write(temperature, 1)\n",
    "    \n",
    "    datasets['temperature'] = temp_path\n",
    "    \n",
    "    # Dataset 3: Vegetation index (NDVI-like)\n",
    "    vegetation = np.random.beta(2, 2, (height, width))  # Values between 0 and 1\n",
    "    vegetation = (vegetation - 0.5) * 1.5  # Scale to -0.75 to 0.75 (NDVI-like range)\n",
    "    \n",
    "    # Water areas have no vegetation\n",
    "    vegetation[water_mask] = -1\n",
    "    \n",
    "    veg_path = tempfile.mktemp(suffix='_vegetation.tif')\n",
    "    with rasterio.open(\n",
    "        veg_path, 'w', driver='GTiff', height=height, width=width,\n",
    "        count=1, dtype=vegetation.dtype, crs='EPSG:4326',\n",
    "        transform=transform, nodata=-1\n",
    "    ) as dst:\n",
    "        dst.write(vegetation, 1)\n",
    "    \n",
    "    datasets['vegetation'] = veg_path\n",
    "    \n",
    "    return datasets, transform\n",
    "\n",
    "# Create test datasets\n",
    "test_datasets, common_transform = create_visualization_datasets()\n",
    "\n",
    "print(\"Created visualization test datasets:\")\n",
    "for name, path in test_datasets.items():\n",
    "    print(f\"  {name}: {path}\")\n",
    "\n",
    "# Quick overview of the datasets\n",
    "fig, axes = plt.subplots(1, 3, figsize=(15, 4))\n",
    "\n",
    "dataset_info = [\n",
    "    ('elevation', 'terrain', 'Elevation (m)'),\n",
    "    ('temperature', 'coolwarm', 'Temperature (Â°C)'),\n",
    "    ('vegetation', 'RdYlGn', 'Vegetation Index')\n",
    "]\n",
    "\n",
    "for i, (name, cmap, label) in enumerate(dataset_info):\n",
    "    with rasterio.open(test_datasets[name]) as src:\n",
    "        data = src.read(1)\n",
    "        extent = [src.bounds.left, src.bounds.right, src.bounds.bottom, src.bounds.top]\n",
    "        \n",
    "        # Handle NoData for display\n",
    "        display_data = data.astype(float)\n",
    "        if src.nodata is not None:\n",
    "            display_data[display_data == src.nodata] = np.nan\n",
    "        \n",
    "        im = axes[i].imshow(display_data, extent=extent, cmap=cmap, origin='upper')\n",
    "        axes[i].set_title(f'{name.title()} Data')\n",
    "        axes[i].set_xlabel('Longitude')\n",
    "        if i == 0:\n",
    "            axes[i].set_ylabel('Latitude')\n",
    "        \n",
    "        plt.colorbar(im, ax=axes[i], label=label, shrink=0.8)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
