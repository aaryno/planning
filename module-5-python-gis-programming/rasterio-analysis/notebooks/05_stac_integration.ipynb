    "\n",
    "# Temporal coverage by month\n",
    "plt.subplot(2, 3, 3)\n",
    "months = range(1, 13)\n",
    "all_monthly = [sum(1 for d in all_dates if d.month == m) for m in months]\n",
    "summer_monthly = [sum(1 for d in summer_df['datetime'] if d.month == m) for m in months] if len(summer_df) > 0 else [0] * 12\n",
    "\n",
    "plt.bar(months, all_monthly, alpha=0.5, label='All scenes', color='gray')\n",
