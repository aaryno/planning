    "# Example 3: RGB and False Color Composites\n",
    "def example_rgb_composites():\n",
    "    \"\"\"\n",
    "    Demonstrate natural color and false color composite creation\n",
    "    \"\"\"\n",
    "    \n",
    "    print(\"\\n=== RGB AND FALSE COLOR COMPOSITES ===\")\n",
    "    \n",
    "    # Load all bands\n",
    "    with rasterio.open(sample_raster) as src:\n",
    "        blue = src.read(1).astype(float)\n",
    "        green = src.read(2).astype(float)\n",
    "        red = src.read(3).astype(float)\n",
    "        nir = src.read(4).astype(float)\n",
    "        transform = src.transform\n",
    "        bounds = src.bounds\n",
    "    \n",
    "    extent = [bounds.left, bounds.right, bounds.bottom, bounds.top]\n",
    "    \n",
    "    # Function to normalize bands for RGB display\n",
    "    def normalize_bands(bands, stretch='percentile'):\n",
    "        normalized = []\n",
    "        for band in bands:\n",
    "            if stretch == 'percentile':\n",
    "                vmin, vmax = np.percentile(band, [2, 98])\n",
    "            else:\n",
    "                vmin, vmax = band.min(), band.max()\n",
    "            \n",
    "            norm_band = (band - vmin) / (vmax - vmin)\n",
    "            norm_band = np.clip(norm_band, 0, 1)\n",
    "            normalized.append(norm_band)\n",
    "        \n",
    "        return normalized\n",
    "    \n",
    "    # Create different composites\n",
