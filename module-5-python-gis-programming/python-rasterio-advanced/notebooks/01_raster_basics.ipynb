   "source": [
    "## 5. Working with Multispectral Imagery\n",
    "\n",
    "Now let's explore the synthetic Landsat-like imagery and calculate vegetation indices."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "landsat_path = 'data/raster/phoenix_landsat.tif'\n",
    "\n",
    "if Path(landsat_path).exists():\n",
    "    with rasterio.open(landsat_path) as src:\n",
    "        print(\"üõ∞Ô∏è Landsat-like Imagery Metadata:\")\n",
    "        print(f\"Dimensions: {src.width} x {src.height} x {src.count} bands\")\n",
    "        print(f\"Data type: {src.dtypes[0]}\")\n",
    "        print(f\"Bounds: {src.bounds}\")\n",
    "        \n",
    "        # Read all bands\n",
    "        blue = src.read(1)   # Band 1: Blue\n",
    "        green = src.read(2)  # Band 2: Green  \n",
    "        red = src.read(3)    # Band 3: Red\n",
    "        nir = src.read(4)    # Band 4: NIR\n",
    "        swir1 = src.read(5)  # Band 5: SWIR1\n",
    "        swir2 = src.read(6)  # Band 6: SWIR2\n",
    "        \n",
    "        # Calculate NDVI (Normalized Difference Vegetation Index)\n",
    "        # NDVI = (NIR - Red) / (NIR + Red)\n",
    "        with np.errstate(divide='ignore', invalid='ignore'):\n",
    "            ndvi = (nir.astype(float) - red.astype(float)) / (nir.astype(float) + red.astype(float))\n",
    "            ndvi = np.where(np.isfinite(ndvi), ndvi, np.nan)\n",
    "        \n",
    "        print(f\"\\nüå± NDVI Statistics:\")\n",
    "        valid_ndvi = ndvi[~np.isnan(ndvi)]\n",
    "        print(f\"Min NDVI: {np.min(valid_ndvi):.3f}\")\n",
    "        print(f\"Max NDVI: {np.max(valid_ndvi):.3f}\")\n",
    "        print(f\"Mean NDVI: {np.mean(valid_ndvi):.3f}\")\n",
    "        \n",
    "        # NDVI interpretation\n",
    "        water_bare = np.sum(valid_ndvi < 0.1) / len(valid_ndvi) * 100\n",
    "        sparse_veg = np.sum((valid_ndvi >= 0.1) & (valid_ndvi < 0.3)) / len(valid_ndvi) * 100\n",
    "        moderate_veg = np.sum((valid_ndvi >= 0.3) & (valid_ndvi < 0.6)) / len(valid_ndvi) * 100\n",
    "        dense_veg = np.sum(valid_ndvi >= 0.6) / len(valid_ndvi) * 100\n",
    "        \n",
    "        print(f\"\\nüèúÔ∏è Land Cover Analysis:\")\n",
    "        print(f\"Water/Bare soil (<0.1): {water_bare:.1f}%\")\n",
    "        print(f\"Sparse vegetation (0.1-0.3): {sparse_veg:.1f}%\")\n",
    "        print(f\"Moderate vegetation (0.3-0.6): {moderate_veg:.1f}%\")\n",
    "        print(f\"Dense vegetation (>0.6): {dense_veg:.1f}%\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Visualize the multispectral data\n",
    "if Path(landsat_path).exists():\n",
    "    fig, axes = plt.subplots(2, 3, figsize=(18, 12))\n",
    "    \n",
    "    # Create RGB composite (scaled for display)\n",
    "    rgb = np.stack([\n",
    "        np.clip(red / np.percentile(red[red > 0], 98) * 255, 0, 255),\n",
    "        np.clip(green / np.percentile(green[green > 0], 98) * 255, 0, 255),\n",
    "        np.clip(blue / np.percentile(blue[blue > 0], 98) * 255, 0, 255)\n",
    "    ], axis=2).astype(np.uint8)\n",
    "    \n",
    "    # False color composite (NIR, Red, Green)\n",
    "    false_color = np.stack([\n",
    "        np.clip(nir / np.percentile(nir[nir > 0], 98) * 255, 0, 255),\n",
    "        np.clip(red / np.percentile(red[red > 0], 98) * 255, 0, 255),\n",
    "        np.clip(green / np.percentile(green[green > 0], 98) * 255, 0, 255)\n",
    "    ], axis=2).astype(np.uint8)\n",
    "    \n",
    "    # Plot RGB composite\n",
    "    axes[0,0].imshow(rgb)\n",
    "    axes[0,0].set_title('True Color (RGB)', fontsize=14, fontweight='bold')\n",
    "    axes[0,0].axis('off')\n",
    "    \n",
    "    # Plot False Color composite\n",
    "    axes[0,1].imshow(false_color)\n",
    "    axes[0,1].set_title('False Color (NIR-Red-Green)', fontsize=14, fontweight='bold')\n",
    "    axes[0,1].axis('off')\n",
    "    \n",
    "    # Plot NDVI\n",
    "    im_ndvi = axes[0,2].imshow(ndvi, cmap='RdYlGn', vmin=-0.2, vmax=0.8)\n",
    "    axes[0,2].set_title('NDVI (Vegetation Index)', fontsize=14, fontweight='bold')\n",
    "    axes[0,2].axis('off')\n",
    "    cbar_ndvi = plt.colorbar(im_ndvi, ax=axes[0,2], shrink=0.8)\n",
    "    cbar_ndvi.set_label('NDVI', rotation=270, labelpad=20)\n",
    "    \n",
    "    # Individual band plots\n",
    "    bands_data = [red, nir, swir1]\n",
    "    band_names = ['Red Band', 'NIR Band', 'SWIR1 Band']\n",
    "    \n",
    "    for i, (band_data, band_name) in enumerate(zip(bands_data, band_names)):\n",
    "        im = axes[1,i].imshow(band_data, cmap='viridis')\n",
    "        axes[1,i].set_title(band_name, fontsize=14, fontweight='bold')\n",
    "        axes[1,i].axis('off')\n",
    "        cbar = plt.colorbar(im, ax=axes[1,i], shrink=0.8)\n",
    "        cbar.set_label('Digital Number', rotation=270, labelpad=20)\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    print(\"‚úÖ Multispectral visualization complete!\")\n",
    "    print(\"Note: Vegetation appears red in false color, water/urban areas appear dark in NDVI.\")"
   ]
  },
