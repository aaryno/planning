    "plt.imshow(water_display, cmap='Blues', origin='lower')\n",
    "plt.colorbar(label='Water Presence')\n",
    "plt.title('Water Mask\\n(NDVI < 0.1)')\n",
    "\n",
    "plt.subplot(2, 3, 3)\n",
    "veg_display = derived_masks['vegetation'].astype(int)\n",
    "plt.imshow(veg_display, cmap='Greens', origin='lower')\n",
    "plt.colorbar(label='Vegetation Presence')\n",
    "plt.title('Vegetation Mask\\n(NDVI > 0.3)')\n",
    "\n",
    "plt.subplot(2, 3, 4)\n",
    "bare_display = derived_masks['bare_soil'].astype(int)\n",
    "plt.imshow(bare_display, cmap='Oranges', origin='lower')\n",
    "plt.colorbar(label='Bare Soil Presence')\n",
    "plt.title('Bare Soil Mask\\n(0.1 ≤ NDVI ≤ 0.3)')\n",
    "\n",
    "# NDVI histogram by land cover\n",
    "plt.subplot(2, 3, 5)\n",
    "# Use original reference masks for analysis\n",
    "forest_mask_ref, agriculture_mask_ref, water_mask_ref, urban_mask_ref = land_cover_masks\n",
    "\n",
    "# Get NDVI values for each reference land cover type\n",
    "forest_ndvi = ndvi[forest_mask_ref][np.isfinite(ndvi[forest_mask_ref])]\n",
    "ag_ndvi = ndvi[agriculture_mask_ref][np.isfinite(ndvi[agriculture_mask_ref])]\n",
    "water_ndvi = ndvi[water_mask_ref][np.isfin
