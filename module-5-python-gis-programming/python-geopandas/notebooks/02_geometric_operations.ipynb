    "    \"\"\"\n",
    "    # Calculate centroids (geometric center)\n",
    "    centroids_gdf = gdf.copy()\n",
    "    centroids_gdf['geometry'] = gdf.geometry.centroid\n",
    "    \n",
    "    # Calculate representative points (guaranteed inside polygon)\n",
    "    repr_points_gdf = gdf.copy()\n",
    "    repr_points_gdf['geometry'] = gdf.geometry.representative_point()\n",
    "    \n",
    "    return {\n",
    "        'centroids': centroids_gdf,\n",
    "        'representative_points': repr_points_gdf\n",
    "    }\n",
