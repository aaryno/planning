    "# Implement basic vegetation indices with error handling\n",
    "def calculate_ndvi_safe(nir, red):\n",
    "    \"\"\"Calculate NDVI with proper error handling.\"\"\"\n",
    "    # Ensure float precision\n",
    "    nir = nir.astype(np.float64)\n",
    "    red = red.astype(np.float64)\n",
    "    \n",
    "    # Calculate denominator\n",
    "    denominator = nir + red\n",
    "    \n",
    "    # Create NDVI array initialized with NaN\n",
    "    ndvi = np.full_like(nir, np.nan)\n",
    "    \n",
    "    # Only calculate where denominator is not zero and both bands are valid\n",
    "    valid_mask = (denominator != 0) & (nir > 0) & (red > 0) & np.isfinite(nir) & np.isfinite(red)\n",
    "    ndvi[valid_mask] = (nir[valid_mask] - red[valid_mask]) / denominator[valid_mask]\n",
    "    \n",
    "    return ndvi\n",
    "\n",
    "def calculate_evi_safe(nir, red, blue):\n",
    "    \"\"\"Calculate EVI with proper error handling.\"\"\"\n",
    "    # EVI coefficients\n",
    "    G = 2.5\n",
    "    C1 = 6.0\n",
    "    C2 = 7.5\n",
    "    L = 1.0\n",
    "    \n",
    "    # Ensure float precision\n",
    "    nir = nir.astype(np.float64)\n",
    "    red = red.astype(np.float64)\n",
    "    blue = blue.astype(np.float64)\n",
    "    \n",
    "    # Calculate denominator\n",
    "    denominator = nir + C1 * red - C2 * blue + L\n",
    "    \n",
    "    # Create EVI array initialized with NaN\n",
    "    evi = np.full_like(nir, np.nan)\n",
    "    \n",
    "    # Only calculate where denominator is not zero and all bands are valid\n",
    "    valid_mask = (denominator != 0) & (nir > 0) & (red > 0) & (blue > 0) & \\\n",
    "                 np.isfinite(nir) & np.isfinite(red) & np.isfinite(blue)\n",
    "    \n",
    "    evi[valid_mask] = G * (nir[valid_mask] - red[valid_mask]) / denominator[valid_mask]\n",
    "    \n",
    "    return evi\n",
    "\n",
    "# Calculate vegetation indices for our sample data\n",
    "ndvi = calculate_ndvi_safe(nir_band, red_band)\n",
    "evi = calculate_evi_safe(nir_band, red_band, blue_band)\n",
    "\n",
    "# Visualize results\n",
    "plt.figure(figsize=(15, 5))\n",
    "\n",
    "plt.subplot(1, 3, 1)\n",
    "plt.imshow(ndvi, cmap='RdYlGn', vmin=-1, vmax=1, origin='lower')\n",
    "plt.colorbar(label='NDVI')\n",
    "plt.title('Normalized Difference\\nVegetation Index (NDVI)')\n",
    "plt.xlabel('X (pixels)')\n",
    "plt.ylabel('Y (pixels)')\n",
    "\n",
    "plt.subplot(1, 3, 2)\n",
    "plt.imshow(evi, cmap='RdYlGn', vmin=-1, vmax=1, origin='lower')\n",
    "plt.colorbar(label='EVI')\n",
    "plt.title('Enhanced\\nVegetation Index (EVI)')\n",
    "plt.xlabel('X (pixels)')\n",
    "plt.ylabel('Y (pixels)')\n",
    "\n",
    "plt.subplot(1, 3, 3)\n",
    "# Compare NDVI vs EVI\n",
    "valid_pixels = np.isfinite(ndvi) & np.isfinite(evi)\n",
    "plt.scatter(ndvi[valid_pixels], evi[valid_pixels], alpha=0.5, s=1)\n",
    "plt.plot([-1, 1], [-1, 1], 'r--', alpha=0.7, label='1:1 Line')\n",
    "plt.xlabel('NDVI')\n",
    "plt.ylabel('EVI')\n",
    "plt.title('NDVI vs EVI Comparison')\n",
    "plt.grid(True, alpha=0.3)\n",
    "plt.legend()\n",
    "plt.axis('equal')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
