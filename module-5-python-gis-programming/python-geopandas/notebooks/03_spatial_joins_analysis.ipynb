        "    if source_gdf.crs and source_gdf.crs.is_geographic:\n",
        "        print(\"‚ö†Ô∏è Reprojecting to Oregon State Plane for accurate distances...\")\n",
        "        source_proj = source_gdf.to_crs('EPSG:2270')\n",
        "        target_proj = target_gdf.to_crs('EPSG:2270')\n",
        "    else:\n",
        "        source_proj = source_gdf.copy()\n",
        "        target_proj = target_gdf.copy()\n",
        "    \n",
        "    # Step 2: Find nearest neighbors using sjoin_nearest (GeoPandas 0.10+)\n",
        "    try:\n",
        "        # Modern method with sjoin_nearest\n",
        "        nearest = source_proj.sjoin_nearest(target_proj, distance_col='distance_m')\n",
        "    except AttributeError:\n",
        "        # Fallback for older GeoPandas versions\n",
