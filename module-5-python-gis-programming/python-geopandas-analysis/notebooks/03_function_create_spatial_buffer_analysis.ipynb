    "# Plot original features on top\n",
    "features_display.plot(ax=ax2, color='red', markersize=100, linewidth=3)\n",
    "\n",
    "ax2.set_title(f'Features with {test_buffer_distance}m Buffer Zones\\nTotal coverage: {merged_area_km2:.6f} km²', \n",
    "             fontsize=12, fontweight='bold')\n",
    "ax2.set_xlabel('Longitude (degrees)')\n",
    "ax2.set_ylabel('Latitude (degrees)')\n",
    "ax2.grid(True, alpha=0.3)\n",
    "\n",
    "# Add legend\n",
    "from matplotlib.patches import Patch\n",
    "legend_elements = [\n",
    "    Patch(facecolor='red', label='Original Features'),\n",
    "    Patch(facecolor='lightblue', alpha=0.5, label=f'{test_buffer_distance}m Buffer Zones')\n",
    "]\n",
    "ax2.legend(handles=legend_elements, loc='upper right')\n",
    "\n",
    "# Add summary statistics as text\n",
    "stats_text = f\"Buffer Statistics:\\n\" \\\n",
    "            f\"• Total area: {total_buffer_area_km2:.6f} km²\\n\" \\\n",
    "            f\"• Coverage area: {merged_area_km2:.6f} km²\\n\" \\\n",
    "            f\"• Overlaps: {overlap_count} pairs\\n\" \\\n",
    "            f\"• Overlap: {overlap_percentage:.1f}%\"\n",
    "\n",
    "ax2.text(0.02, 0.98, stats_text, transform=ax2.transAxes, \n",
    "        bbox=dict(boxstyle='round', facecolor='white', alpha=0.8),\n",
    "        verticalalignment='top', fontsize=9)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(\"✅ Buffer analysis visualization created successfully!\")"
   ]
  },
