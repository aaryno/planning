    "# Implement vegetation health assessment\n",
    "def classify_vegetation_health(ndvi):\n",
    "    \"\"\"Classify vegetation health based on NDVI values.\"\"\"\n",
    "    health_class = np.full_like(ndvi, np.nan)\n",
    "    \n",
    "    valid_mask = np.isfinite(ndvi)\n",
    "    \n",
    "    # Apply classification\n",
    "    health_class[valid_mask & (ndvi < 0.1)] = 0  # Water/No vegetation\n",
    "    health_class[valid_mask & (ndvi >= 0.1) & (ndvi < 0.3)] = 1  # Bare soil/sparse\n",
    "    health_class[valid_mask & (ndvi >= 0.3) & (ndvi < 0.5)] = 2  # Low vegetation\n",
    "    health_class[valid_mask & (ndvi >= 0.5) & (ndvi < 0.7)] = 3  # Moderate vegetation\n",
    "    health_class[valid_mask & (ndvi >= 0.7)] = 4  # Dense vegetation\n",
    "    \n",
    "    return health_class.astype(np.int16)\n",
    "\n",
    "def create_land_cover_masks(ndvi, evi=None):\n",
    "    \"\"\"Create land cover masks using vegetation indices.\"\"\"\n",
    "    masks = {}\n",
    "    \n",
    "    # Water mask: Very low NDVI\n",
    "    masks['water'] = ndvi < 0.1\n",
    "    \n",
    "    # Vegetation mask: Moderate to high NDVI\n",
    "    masks['vegetation'] = ndvi > 0.3\n",
    "    \n",
    "    # Bare soil mask: Low positive NDVI\n",
    "    masks['bare_soil'] = (ndvi >= 0.1) & (ndvi <= 0.3)\n",
    "    \n",
    "    # If EVI is available, refine masks\n",
    "    if evi is not None:\n",
    "        # Dense vegetation: High NDVI and EVI\n",
    "        masks['dense_vegetation'] = (ndvi > 0.6) & (evi > 0.4)\n",
    "        \n",
    "        # Sparse vegetation: Moderate indices\n",
    "        masks['sparse_vegetation'] = (ndvi > 0.3) & (ndvi <= 0.6) & (evi > 0.2)\n",
    "    \n",
    "    return masks\n",
    "\n",
    "def assess_seasonal_suitability(ndvi):\n",
    "    \"\"\"Assess seasonal suitability for agricultural activities.\"\"\"\n",
    "    suitability = {\n",
    "        'planting': np.sum((ndvi >= 0.2) & (ndvi < 0.4)) / np.sum(np.isfinite(ndvi)),\n",
    "        'growing': np.sum((ndvi >= 0.4) & (ndvi < 0.8)) / np.sum(np.isfinite(ndvi)),\n",
    "        'harvest': np.sum((ndvi >= 0.3) & (ndvi < 0.6)) / np.sum(np.isfinite(ndvi))\n",
    "    }\n",
    "    return suitability\n",
    "\n",
    "# Apply classification and analysis\n",
    "health_classification = classify_vegetation_health(ndvi)\n",
    "derived_masks = create_land_cover_masks(ndvi, evi)\n",
    "seasonal_suitability = assess_seasonal_suitability(ndvi)\n",
    "\n",
    "# Visualize results\n",
    "plt.figure(figsize=(18, 12))\n",
    "\n",
    "# Vegetation health classification\n",
    "plt.subplot(2, 3, 1)\n",
    "health_colors = ['darkblue', 'brown', 'orange', 'lightgreen', 'darkgreen']\n",
    "health_cmap = ListedColormap(health_colors)\n",
    "plt.imshow(health_classification, cmap=health_cmap, origin='lower')\n",
    "plt.colorbar(ticks=[0,1,2,3,4], label='Health Class')\n",
    "plt.title('Vegetation Health Classification')\n",
    "# Add legend\n",
    "labels = ['Water', 'Bare/Sparse', 'Low Veg', 'Mod Veg', 'Dense Veg']\n",
    "for i, (label, color) in enumerate(zip(labels, health_colors)):\n",
    "    plt.text(2, 95-i*4, f'{i}: {label}', color=color, fontweight='bold', fontsize=9)\n",
    "\n",
    "# Land cover masks\n",
