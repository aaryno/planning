   "source": [
    "## ðŸ“Š Step 6: Advanced Visualization Techniques\n",
    "\n",
    "Learn some advanced techniques for better raster visualization:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Advanced visualization techniques\n",
    "with rasterio.open(elevation_file) as src:\n",
    "    band_data = src.read(1)\n",
    "    \n",
    "    # Handle NoData\n",
    "    if src.nodata is not None:\n",
    "        display_data = np.ma.masked_equal(band_data, src.nodata)\n",
    "        valid_data = band_data[band_data != src.nodata]\n",
    "    else:\n",
    "        display_data = band_data\n",
    "        valid_data = band_data.flatten()\n",
    "    \n",
    "    # Create advanced visualization with multiple techniques\n",
    "    fig = plt.figure(figsize=(16, 12))\n",
    "    \n",
    "    # 1. Standard visualization\n",
    "    ax1 = plt.subplot(2, 2, 1)\n",
    "    im1 = ax1.imshow(display_data, cmap='terrain',\n",
    "                    extent=[src.bounds.left, src.bounds.right, \n",
    "                           src.bounds.bottom, src.bounds.top])\n",
    "    ax1.set_title('Standard Visualization')\n",
    "    ax1.set_xlabel('Longitude')\n",
    "    ax1.set_ylabel('Latitude')\n",
    "    plt.colorbar(im1, ax=ax1, shrink=0.8)\n",
    "    \n",
    "    # 2. Histogram equalization (better contrast)\n",
    "    ax2 = plt.subplot(2, 2, 2)\n",
    "    # Normalize data to 0-1 range for better visualization\n",
    "    if len(valid_data) > 0:\n",
    "        data_min, data_max = np.min(valid_data), np.max(valid_data)\n",
    "        normalized_data = (display_data - data_min) / (data_max - data_min)\n",
    "        im2 = ax2.imshow(normalized_data, cmap='terrain',\n",
    "                        extent=[src.bounds.left, src.bounds.right, \n",
    "                               src.bounds.bottom, src.bounds.top])\n",
    "        ax2.set_title('Normalized (0-1 Range)')\n",
    "    else:\n",
    "        im2 = ax2.imshow(display_data, cmap='terrain',\n",
    "                        extent=[src.bounds.left, src.bounds.right, \n",
    "                               src.bounds.bottom, src.bounds.top])\n",
    "        ax2.set_title('Standard (No Valid Data)')\n",
    "    ax2.set_xlabel('Longitude')\n",
    "    ax2.set_ylabel('Latitude')\n",
    "    plt.colorbar(im2, ax=ax2, shrink=0.8)\n",
    "    \n",
    "    # 3. Custom color limits (focusing on data range)\n",
    "    ax3 = plt.subplot(2, 2, 3)\n",
    "    if len(valid_data) > 0:\n",
    "        # Use percentiles to set limits (removes outliers)\n",
    "        vmin, vmax = np.percentile(valid_data, [2, 98])  # 2nd to 98th percentile\n",
    "        im3 = ax3.imshow(display_data, cmap='terrain', vmin=vmin, vmax=vmax,\n",
    "                        extent=[src.bounds.left, src.bounds.right, \n",
    "                               src.bounds.bottom, src.bounds.top])\n",
    "        ax3.set_title(f'Percentile Stretch (2%-98%)')\n",
    "    else:\n",
    "        im3 = ax3.imshow(display_data, cmap='terrain',\n",
    "                        extent=[src.bounds.left, src.bounds.right, \n",
    "                               src.bounds.bottom, src.bounds.top])\n",
    "        ax3.set_title('Standard (No Valid Data)')\n",
    "    ax3.set_xlabel('Longitude')\n",
    "    ax3.set_ylabel('Latitude')\n",
    "    plt.colorbar(im3, ax=ax3, shrink=0.8)\n",
    "    \n",
    "    # 4. Contour overlay (for elevation data)\n",
    "    ax4 = plt.subplot(2, 2, 4)\n",
    "    im4 = ax4.imshow(display_data, cmap='terrain', alpha=0.8,\n",
    "                    extent=[src.bounds.left, src.bounds.right, \n",
    "                           src.bounds.bottom, src.bounds.top])\n",
    "    \n",
    "    # Add contour lines if we have elevation data\n",
    "    if len(valid_data) > 0 and 'elevation' in str(elevation_file).lower():\n",
    "        # Create coordinate arrays\n",
    "        x = np.linspace(src.bounds.left, src.bounds.right, src.width)\n",
    "        y = np.linspace(src.bounds.bottom, src.bounds.top, src.height)\n",
    "        X, Y = np.meshgrid(x, y)\n",
    "        \n",
    "        # Add contour lines every 100m (adjust for your data)\n",
    "        data_range = np.max(valid_data) - np.min(valid_data)\n",
    "        num_contours = min(10, max(5, int(data_range / 100)))  # Reasonable number of contours\n",
    "        \n",
    "        # Only add contours if we have valid data\n",
    "        if not np.ma.is_masked(display_data) or not display_data.mask.all():\n",
    "            contours = ax4.contour(X, Y, np.flipud(display_data), \n",
    "                                 levels=num_contours, colors='white', alpha=0.6, linewidths=0.8)\n",
    "            ax4.clabel(contours, inline=True, fontsize=8, colors='white')\n",
    "    \n",
    "    ax4.set_title('With Contour Lines')\n",
    "    ax4.set_xlabel('Longitude')\n",
    "    ax4.set_ylabel('Latitude')\n",
    "    plt.colorbar(im4, ax=ax4, shrink=0.8)\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    print(\"ðŸŽ¨ ADVANCED VISUALIZATION TECHNIQUES:\")\n",
    "    print(\"â€¢ Standard: Direct pixel value mapping\")\n",
    "    print(\"â€¢ Normalized: Scale data to 0-1 for better contrast\")\n",
    "    print(\"â€¢ Percentile Stretch: Remove outliers for better visualization\")\n",
    "    print(\"â€¢ Contour Overlay: Add elevation contours for better interpretation\")\n",
    "    print(\"ðŸ’¡ Choose the technique that best highlights your data patterns!\")"
